{
  "manifest_version": 2,

  "name": "Shortcut Menu 'search by date' for Google.",
  "description": "This extension display a 'search by date' menu on result page.",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["jquery-3.2.1.min.js", "myscript.js"]

    }
  ],
  "permissions": [
  ]
}
